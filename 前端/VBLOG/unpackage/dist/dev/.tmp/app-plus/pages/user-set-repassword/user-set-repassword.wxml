<view class="body"><input class="uni-input common-input" type="text" placeholder="输入新密码" data-event-opts="{{[['input',[['__set_model',['','newPassword','$event',[]]]]]]}}" value="{{newPassword}}" bindinput="__e"/><input class="uni-input common-input" type="text" placeholder="输入确认密码" data-event-opts="{{[['input',[['__set_model',['','renewPassword','$event',[]]]]]]}}" value="{{renewPassword}}" bindinput="__e"/><button class="{{['user-set-btn',(disabled)?'user-set-btn-disable':'']}}" loading="{{loading}}" type="primary" disabled="{{disabled}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">完成</button></view>